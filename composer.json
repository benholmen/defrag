{
    "name": "benholmen/defrag",
    "description": "Defragment your PHPUnit tests",
    "keywords": [
        "benholmen",
        "defrag"
    ],
    "homepage": "https://github.com/benholmen/defrag",
    "license": "MIT",
    "authors": [
        {
            "name": "Ben Holmen",
            "email": "ben@benholmen.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1"
    },
    "require-dev": {
        "illuminate/collections": "^10.0|^11.0",
        "laravel/pint": "^1.0",
        "laravel/prompts": "^0.1",
        "phpunit/phpunit": "^10.3",
        "spatie/ray": "^1.0",
        "symfony/console": "^6.0"
    },
    "autoload": {
        "psr-4": {
            "BenHolmen\\Defrag\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "BenHolmen\\Defrag\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
            "test-coverage": "vendor/bin/phpunit --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "phpstan/extension-installer": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
